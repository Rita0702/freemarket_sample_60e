.wrapper#__next
  -# ここからヘッダーです
  = render 'shared/header'
  -# ここまでヘッダーです
  %section.item-box-container.l-single-container
    %h1.item-name
      = @product.name
    .item-main-content.clearfix
      .item-photo
        .owl-carousel.owl-loaded.owl-drag
          .owl-stage-outer
            .owl-stage
              .owl-item{style: "width: 300px;"}
                .owl-item-inner
                  %div
                    = image_tag "#{@product.images[0].image_url}", alt: 'image', style: "opacity: 1;", class: 'owl-lazy'
                    -if @product.buyer == "1" 
                      .items-box_photos__sold
                        .items-box_photos__sold__inner
                          SOLD
          .owl-nav.disabled
            .owl-prev 
            .owl-next 
          .owl-dots
            .owl-dot
              %span
      %section.visible-sp
        .item-btn-float-area
      %table.item-detail-table
        %tbody
          %tr
            %th 出品者
            %td
              = link_to 'ブランド多数出品中！', '#'
              %div
                .item-user-ratings
                  %i.far.fa-smile
                  %span 656
                .item-user-ratings
                  %i.far.fa-meh
                  %span 13
                .item-user-ratings
                  %i.far.fa-frown
                  %span 5
          %tr
            %th カテゴリー
            %td
              = link_to '#' do
                %div 
                  = @product.category.name
              = link_to '#' do
                .item-detail-table-sub-category
                  %i.fas.fa-chevron-right
                  小物
              = link_to '#' do
                .item-detail-table-sub-sub-category
                  %i.fas.fa-chevron-right
                  長財布
          %tr
            %th ブランド
            %td
              = link_to '#' do
                %div
                  = @product.brand.name
          %tr
            %th 商品の状態
            %td 
              = @product.status.name
          %tr
            %th 配送料の負担
            %td 
              = @product.delivery_fee
          %tr
            %th 配送の方法
            %td 
              = @product.delivery
          %tr
            %th 配送元地域
            %td
              = link_to '#' do
                = @product.shipping_area
          %tr
            %th 発送日の目安
            %td 
              = @product.shipping_days
    .item-price-box.text-center
      %span.item-price.bold 
        = "¥ " + @product.price
      %span.item-tax (税込)
      %span.item-shipping-fee 
        = @product.delivery_fee
    -if @product.buyer == "1" 
      .item-buy-btns
        取引停止中
    -else
      = link_to '購入画面に進む', "/products/buy/#{@product.id}", class: "item-buy-btn" 
    .item-description.f14
      %p.item-description-inner
        = @product.description
    .item-button-container.clearfix
      .item-button-left
        = link_to '#', class: "item-button item-button-like" do
          %i.far.fa-heart
          %span いいね!
          %span{"data-num": "like"} 11
        = link_to '#', class: "item-button item-button-report clearfix" do
          %i.far.fa-flag
          %span 不適切な商品の報告
      .item-button-right
        = link_to '#' do
          %i.fas.fa-lock
          %span あんしん・あんぜんへの取り組み
  .item-detail-message
